<p style="color: green"><%= notice %></p>

<h1>Quizzes</h1>

<div class="btn-group">
  <%= link_to "New quiz", new_quiz_path, class: "btn" %>
</div>

<table>
  <thead>
    <tr>
      <th>Quiz</th>
      <th>Created By</th>
      <th>Created At</th>
      <th>Updated At</th>
    </tr>
  </thead>
  <tbody>
    <% @quizzes.each do |quiz| %>
      <tr>
        <td>
          <%= link_to quiz %>
        </td>
        <td>
          <%= quiz.user %>
        </td>
        <td>
          <%= quiz.created_at %>
        </td>
        <td>
          <%= quiz.updated_at %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

